15},"source":"components/Block.jsx","original":{"line":53,"column":15}}],"sources":{"components/Block.jsx":"import React, { useState, useEffect } from \"react\";\nimport { Button } from \"react-bootstrap\";\nimport Transaction from \"./Transaction\";\n\nfunction Block({ timestamp, hash, data }) {\n  const [toggleTransaction, setToggleTransaction] = useState(false);\n\n  const hashDisplay = `${hash.substring(0, 15)}...`;\n  const stringifiedData = JSON.stringify(data);\n\n  const displayTransaction = () => {\n    const dataDisplay =\n      stringifiedData.length > 35\n        ? `${stringifiedData.substring(0, 35)}...`\n        : stringifiedData;\n\n    if (toggleTransaction) {\n      return (\n        <div>\n          {data.map((transaction) => (\n            <div key={transaction.id}>\n              <hr />\n              <Transaction transaction={transaction} />\n            </div>\n          ))}\n          <br />\n          <Button onClick={() => setToggleTransaction(!toggleTransaction)}>\n            Show Less\n          </Button>\n        </div>\n      );\n    }\n\n    return (\n      <div>\n        <div>Data: {dataDisplay}</div>\n        <Button onClick={() => setToggleTransaction(!toggleTransaction)}>\n          Show More\n        </Button>\n      </div>\n    );\n  };\n\n  return (\n    <div className=\"block\">\n      <div>Hash: {hashDisplay}</div>\n      <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\n      {displayTransaction()}\n    </div>\n  );\n}\n\nexport default Block;\n"},"lineCount":null}},"error":null,"hash":"661a291ba48b7bc26a4cfe8e4f19befb","cacheData":{"env":{}}}